{
		"name": "cv.joellesenne.dev",
		"version": "1.0.1",
		"description": "My online CV using Gatsby.js",
		"keywords": [
				"gatsby"
		],
		"homepage": "https://cv.joellesenne.dev",
		"bugs": {
				"url": "https://github.com/joellesenne/cv.joellesenne.dev/issues"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/joellesenne/cv.joellesenne.dev.git"
		},
		"license": "MIT",
		"author": "JoÃ«l Lesenne <contact@joellesenne.dev>",
		"scripts": {
				"build": "gatsby build",
				"develop": "gatsby develop -o",
				"clean": "rm -rf .cache & rm -rf public",
				"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
				"lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
				"lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
				"prepare": "yarn run snyk-protect",
				"serve": "gatsby serve",
				"snyk-protect": "snyk protect",
				"start": "npm run develop",
				"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
		},
		"eslintConfig": {
				"env": {
						"browser": true,
						"node": true
				}
		},
		"dependencies": {
				"axios": "0.24.0",
				"gatsby": "4.3.0",
				"gatsby-image": "3.11.0",
				"gatsby-plugin-google-analytics": "4.3.0",
				"gatsby-plugin-manifest": "4.3.0",
				"gatsby-plugin-offline": "5.3.0",
				"gatsby-plugin-react-helmet": "5.3.0",
				"gatsby-plugin-robots-txt": "1.6.14",
				"gatsby-plugin-theme-ui": "0.13.1",
				"gatsby-source-filesystem": "4.3.0",
				"prop-types": "15.7.2",
				"react": "17.0.2",
				"react-dom": "17.0.2",
				"react-helmet": "6.1.0",
				"snyk": "1.790.0",
				"styled-components": "5.3.3",
				"theme-ui": "0.13.1"
		},
		"devDependencies": {
				"@babel/core": "7.16.0",
				"@babel/eslint-parser": "7.16.3",
				"dotenv": "10.0.0",
				"eslint": "8.4.1",
				"eslint-config-airbnb": "19.0.2",
				"eslint-config-prettier": "8.3.0",
				"eslint-plugin-import": "2.25.3",
				"eslint-plugin-jsx-a11y": "6.5.1",
				"eslint-plugin-prettier": "4.0.0",
				"eslint-plugin-react": "7.27.1",
				"eslint-plugin-react-hooks": "4.3.0",
				"gatsby-plugin-root-import": "2.0.8",
				"prettier": "2.5.1"
		},
		"engines": {
				"node": ">=14.0.0",
				"npm": ">=7.0.0"
		}
