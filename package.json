{
		"name": "cv-resume",
		"version": "2.0.0",
		"description": "My online CV using Gatsby.js",
		"keywords": [
				"gatsby"
		],
		"homepage": "https://cv.joellesenne.dev",
		"bugs": {
				"url": "https://github.com/joellesenne/cv-resume/issues"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/joellesenne/cv-resume.git"
		},
		"license": "MIT",
		"author": "JoÃ«l Lesenne <contact@joellesenne.dev>",
		"scripts": {
				"build": "gatsby build",
				"develop": "gatsby develop -o",
				"clean": "rm -rf .cache & rm -rf public",
				"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
				"lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
				"lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
				"serve": "gatsby serve",
				"snyk-prepare": "npm run snyk-protect",
				"snyk-protect": "snyk-protect",
				"start": "npm run develop",
				"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
		},
		"eslintConfig": {
				"env": {
						"browser": true,
						"node": true
				}
		},
		"dependencies": {
				"@emotion/react": "11.10.5",
				"@mdx-js/react": "2.1.5",
				"@theme-ui/mdx": "^0.15.4",
				"avataaars": "^2.0.0",
				"axios": "1.2.1",
				"gatsby": "5.2.0",
				"gatsby-plugin-google-gtag": "5.2.0",
				"gatsby-plugin-image": "3.2.0",
				"gatsby-plugin-manifest": "5.2.0",
				"gatsby-plugin-offline": "6.2.0",
				"gatsby-plugin-react-helmet": "6.2.0",
				"gatsby-plugin-robots-txt": "1.8.0",
				"gatsby-plugin-theme-ui": "0.15.4",
				"gatsby-source-filesystem": "5.2.0",
				"prop-types": "15.8.1",
				"react": "18.2.0",
				"react-dom": "18.2.0",
				"react-helmet": "6.1.0",
				"styled-components": "5.3.6",
				"theme-ui": "0.15.4"
		},
		"devDependencies": {
				"@babel/core": "7.20.5",
				"@babel/eslint-parser": "7.19.1",
				"@babel/preset-env": "^7.20.2",
				"dotenv": "16.0.3",
				"eslint": "8.29.0",
				"eslint-config-airbnb": "19.0.4",
				"eslint-config-prettier": "8.5.0",
				"eslint-plugin-import": "2.26.0",
				"eslint-plugin-jsx-a11y": "6.6.1",
				"eslint-plugin-prettier": "4.2.1",
				"eslint-plugin-react": "7.31.11",
				"eslint-plugin-react-hooks": "4.6.0",
				"gatsby-plugin-pnpm": "^1.2.10",
				"prettier": "2.8.1"
		},
		"engines": {
				"node": ">=14.0.0",
				"npm": ">=7.0.0"
		},
		"pnpm": {
				"overrides": {
						"ansi-html@<0.0.8": ">=0.0.8",
						"node-fetch@<2.6.7": ">=2.6.7",
						"ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
						"immer@<9.0.6": ">=9.0.6",
						"ws@>=7.0.0 <7.4.6": ">=7.4.6",
						"browserslist@>=4.0.0 <4.16.5": ">=4.16.5"
				}
		}
}
